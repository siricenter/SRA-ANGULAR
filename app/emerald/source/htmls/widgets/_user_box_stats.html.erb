<div class="panel panel-success-lt">

  <div class="panel-body no-padding">

    <div class="panel-colorize gradient-diag1">
      <div class="panel-heading">
        <%= partial 'htmls/widgets/actions', locals: { colors: false } %>
        <div class="panel-title">&nbsp;</div>
      </div>

      <div class="hpadded panel-body-collapse">

        <div class="box-table push-bottom">
          <div class="box-row box-row-mid">
            <div class="box-cell">
              <div class="text-semi text-right">
                <div><i class="fa fa-trophy fa-2x"></i></div>
                <div>LEVEL</div>
              </div>
              <div class="text-right">
                INTERMEDIATE
              </div>
            </div>
            <div class="box-cell">
              <h1 class="text-lg no-margin hpadded major">5</h1>
            </div>
          </div>
        </div>

        <div class="row">
          <div class="col-xs-10 col-xs-offset-1">
            <div class="progress flat-progress" rel="tooltip" title="60% complete">
              <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%;">
                <span class="sr-only">60% Complete</span>
              </div>
            </div>
          </div>
        </div>

      </div>

      <div class="padded panel-body-collapse">

        <div class="row">
          <div class="col-xs-6">
            <ul class="list-inline">
              <li class="v-mid">
                <h1 class="no-margin">729</h1>
              </li>
              <li>
                <div class="text-semi">TOTAL</div>
                <div class="text-semi">POINTS</div>
              </li>
            </ul>
          </div>

          <div class="col-xs-6 text-right">
            <ul class="list-inline">
              <li class="v-mid">
                <div class="text-semi">SCORE TO</div>
                <div class="text-semi">LEVEL UP</div>
              </li>
              <li>
                <h1 class="no-margin">442</h1>
              </li>
            </ul>
          </div>
        </div>

      </div>
    </div>

    <div class="padded panel-body-collapse">
      <div class="flot-line" style="height:200px" data-color="success-lt"></div>
    </div>

    <div class="panel-line-top text-center panel-body-collapse">
      <div class="row">
        <div class="col-xs-6 panel-line-right">
          <h4 class="panel-text-dk">561</h4>
          <h5>Performance</h5>
        </div>
        <div class="col-xs-6">
          <h4 class="panel-text-dk">223</h4>
          <h5>To Go</h5>
        </div>
      </div>
    </div>

  </div>
</div>